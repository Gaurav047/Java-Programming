package p1;

public class Addition {
	int num1, num2, num3;

	public void setData(int num1, int num2) {

		this.num1 = num1;
		this.num2 = num2;
	}

	public void readData() {
		System.out.println("Enter the two Numbers : ");
		this.num1 = read.br.nextInt();
		this.num2 = read.br.nextInt();
	}

	public void CalData() {
		num3 = num1 + num2;
	}

	public void display() {
		System.out.printf("%d \n", num3);
	}
}
