package oops.motorbike;

public class MotorBikeRunner {

	public static void main(String[] args) {
		MotorBike Honda = new MotorBike();
		MotorBike Ducati = new MotorBike();

		Ducati.start();
		Honda.start();

		Ducati.speed = 100;
		Honda.speed = 80;

		System.out.printf("Speed of Ducati is : " + Ducati.speed);
		System.out.printf("Speed of Honda is : " + Honda.speed);

	}

}
