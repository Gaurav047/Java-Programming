package oops.motorbike;

public class MotorBikeRunner {

	public static void main(String[] args) {
		MotorBike Honda = new MotorBike();
		MotorBike Ducati = new MotorBike();

		Ducati.start();
		Honda.start();

		Ducati.setSpeed(100);
		Honda.setSpeed(50);

		Ducati.printSpeed();
		Honda.printSpeed();

		Ducati.setSpeed(80);
		Honda.setSpeed(60);

		Ducati.printSpeed();
		Honda.printSpeed();

	}

}
